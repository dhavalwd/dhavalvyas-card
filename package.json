{
  "name": "dhavalvyas",
  "version": "1.0.0",
  "description": "Personal npm card for Dhaval Vyas (@dhavalvyas)",
  "main": "dist/card.js",
  "unpkg": "index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhavalwd/dhavalvyas-card.git"
  },
  "scripts": {
    "prepare": "node src/htmlify",
    "prepublishOnly": "rm -rf dist && webpack"
  },
  "bin": {
    "dhavalvyas": "bin/index.js"
  },
  "files": [
    "bin",
    "dist",
    "index.html"
  ],
  "keywords": [
    "card",
    "npm",
    "npm card",
    "npx",
    "npx card",
    "business card"
  ],
  "author": "Dhaval Vyas",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "ansi-to-html": "^0.6.9",
    "babel-loader": "^8.0.4",
    "boxen": "^2.1.0",
    "chalk": "^2.4.1",
    "chalker": "^1.0.0",
    "lodash.get": "^4.4.2",
    "webpack": "^4.28.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  },
  "myCard": {
    "info": {
      "name": "Dhaval Vyas",
      "location": "Halifax, Canada",
      "handle": "dhavalvyas",
      "twitter": "VyasDhaval_",
      "npm": "dhavalvyas",
      "github": "dhavalwd",
      "linkedin": "dhavalbvyas",
      "web": "https://dhavalvyas.me",
      "work": "Front End Developer",
      "dev": "dhavalwd"
    },
    "data": [
      "          <white><bold>{{name}}</></><green> / </><cyan>{{work}}</>",
      "          <white>{{location}}</>",
      "",
      {
        "label": "Website",
        "text": "<magenta>{{web}}</>",
        "when": "{{web}}"
      },
      {
        "label": "Dev.to",
        "text": "https://dev.to/<magenta>{{dev}}</>",
        "when": "{{dev}}"
      },
      {
        "label": "Twitter",
        "text": "https://twitter.com/<cyan>{{twitter}}</>",
        "when": "{{twitter}}"
      },
      {
        "label": "npm",
        "text": "https://www.npmjs.com/<red>~{{npm}}</>",
        "when": "{{npm}}"
      },
      {
        "label": "GitHub",
        "text": "https://github.com/<green>{{github}}</>",
        "when": "{{github}}"
      },
      {
        "label": "LinkedIn",
        "text": "https://www.linkedin.com/in/<blue>{{linkedin}}</>",
        "when": "{{linkedin}}"
      },
      "",
      {
        "label": "Card",
        "text": "<red>npx</> {{_packageName}}"
      }
    ]
  }
}
